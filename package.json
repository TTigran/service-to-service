{
  "name": "service-to-service",
  "version": "0.0.0",
  "private": true,
  "type": "node",
  "request": "launch",
  "test": "jest",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "runtimeArgs": [
    "-r",
    "ts-node/register"
  ],
  "main": "src/index.ts",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "start": "ts-node src/index.ts",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "formidable": "^1.2.2",
    "http-errors": "~1.6.3",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.7",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "zeronode": "^1.1.35"
  },
  "devDependencies": {
    "@types/supertest": "^2.0.8",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/body-parser": "^1.19.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/mongodb": "^3.5.8",
    "@types/mongoose": "^5.7.11",
    "@types/node": "^13.11.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "superagent": "^5.2.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
}
